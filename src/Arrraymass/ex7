package array1.ControlX.arraysmass;
public class ex7 {
    public static void main(String[] args) {
        int[][] arr = new int[4][4];
        for (int i = 0; i < arr.length; i++) {
            for (int j = 0; j < arr.length; j++) {
                arr[i][j] = j+1;
                System.out.print(arr[i][j] + " ");
            }
            System.out.println();
        }
    }
}
